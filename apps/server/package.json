{
  "name": "server",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot src/index.ts",
    "reset": "bun -e 'const { readdir, unlink } = await import(\"node:fs/promises\"); for (const f of await readdir(\".\")) if (f.match(/\\.sqlite/)) await unlink(f)'",
    "start": "bun reset; bun run src/index.ts"
  },
  "dependencies": {
    "@canary/db": "workspace:*",
    "@canary/env": "workspace:*",
    "@effect/ai": "^0.33.2",
    "@effect/ai-openai": "^0.37.2",
    "@effect/cli": "^0.73.0",
    "@effect/cluster": "^0.56.4",
    "@effect/experimental": "^0.58.0",
    "@effect/opentelemetry": "^0.61.0",
    "@effect/platform": "^0.94.4",
    "@effect/platform-bun": "^0.87.0",
    "@effect/sql": "^0.49.0",
    "@effect/sql-pg": "^0.50.3",
    "@effect/sql-sqlite-bun": "^0.50.2",
    "@effect/workflow": "^0.16.0",
    "@huggingface/transformers": "^3.8.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/sdk-trace-node": "^2.5.0",
    "@types/mdast": "^4.0.4",
    "ansis": "^4.1.0",
    "bullmq": "^5.66.7",
    "dotenv": "catalog:",
    "drizzle-orm": "1.0.0-beta.15-90d1d1a",
    "effect": "^3.19.16",
    "fast-xml-parser": "^5.3.3",
    "remark": "^15.0.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@canary/config": "workspace:*",
    "@types/bun": "catalog:",
    "fast-check": "^4.5.3",
    "mitata": "^1.0.34",
    "tsdown": "^0.16.5",
    "typescript": "catalog:"
  }
}
